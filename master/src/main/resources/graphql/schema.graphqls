type Query {
    proteinById(uniprotid: String!): Protein
    proteins: [Protein]
}

type Protein {
	id: ID!
	uniprotid: String!
	ensembl_ids: String!
	gene_ids: String!
	interacting_proteins1: [IncomingInteraction!]
	interacting_proteins2: [OutgoingInteraction!]
}

type IncomingInteraction {
	id : ID!
	score: String!
	databases: String!
	interactor: Protein!
}

type OutgoingInteraction {
	id : ID!
	score: String!
	databases: String!
	interactor: Protein!
}
